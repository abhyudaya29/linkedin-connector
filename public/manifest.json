{
    "manifest_version": 3,
    "name": "LinkedIn Auto Connector",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
  "default_popup": "index.html",
  "default_icon": "icon.png"
}
,
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["contentScript.js"]
      }
    ]
  }
  